{
  "version": 2,
  "name": "ln-builder",
  "build": {
    "env": {
      "SESSION_SECRET": "@session-secret",
      "OPEN_NODE_KEY": "@open-node-key",
      "CAVEAT_KEY": "@caveat-key"
    }
  },
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/api(.*)",
      "dest": "index.js"
    }
  ]
}
